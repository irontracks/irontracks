-
-                totals.p += p; totals.c += c; totals.f += f; totals.kcal += kcal;
-
-                detectedFoods.push({
-                    name: matchedItem.unit,
-                    rawName: rawLine,
-                    qtd: qtd,
-                    unitDisplay: unitUsed,
-                    macros: { p, c, f, kcal }
-                });
-            }
-            else {
-                // --- STANDARD LOGIC ---
-                if (wasApprox) {
-                    isApproximate = true;
