+export interface ChartDataPoint {
+  date: string;
+  body_fat_percentage: number;
+  lean_mass: number;
+  weight: number;
+}
+
+export interface BodyCompositionData {
+  labels: string[];
+  data: number[];
+  colors: string[];
+}
