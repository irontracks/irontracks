-            if (!entry.normalizedKey) continue;
-            if (foodName.includes(entry.normalizedKey)) {
-                if (!dbKeyMatched || entry.normalizedKeyLength > dbKeyMatched.length) {
-                    dbKeyMatched = entry.normalizedKey;
