+}
+
+export interface CalculatedMetrics {
+  body_fat_percentage: number;
+  lean_mass: number;
+  fat_mass: number;
+  bmr: number;
+  bmi: number;
+  sum_skinfolds: number;
+  body_density: number;
+}
+
+export interface ActivityLevel {
+  value: 'sedentary' | 'light' | 'moderate' | 'active' | 'veryActive';
