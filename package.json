    "@revenuecat/purchases-capacitor": "^12.1.2",
    "@supabase/auth-js": "^2.89.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.87.3",
    "@tailwindcss/node": "^4.2.0",
