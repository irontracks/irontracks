    "prestart": "node scripts/ensure-next-compiled-commander.mjs",
    "dev": "next dev --webpack --hostname localhost --port 3000",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "node --import tsx ./node_modules/eslint/bin/eslint.js --config eslint.config.mjs .",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:smoke": "tsx scripts/delete-teacher-cascade.test.ts && tsx scripts/vip-entitlement.test.ts && tsx scripts/admin-routes-smoke.test.ts && tsx scripts/auth-origin-smoke.test.ts && tsx scripts/no-duplicate-style-files.test.ts && tsx scripts/vip-endpoints-no-dup.test.ts && tsx scripts/no-direct-req-json.test.ts && tsx scripts/workouts-history-enforcement.test.ts && tsx scripts/finish-payload-smoke.test.ts && tsx scripts/critical-flow-smoke.test.ts",
    "flags:report": "NODE_NO_WARNINGS=1 tsx scripts/feature-flags-report.ts",
    "deploy": "npm run lint --silent && npm run test:unit && npm run test:smoke && npm run build && git add -A && (git diff --cached --quiet && echo 'Nenhuma mudanÃ§a para commitar.' || git commit -m \"deploy: $(date +%Y-%m-%d-%H%M)\") && git push origin main",
    "e2e:deload": "tsx scripts/e2e/deload.e2e.ts",
    "postversion": "git push && git push --tags",
    "postinstall": "node scripts/patch-ios.mjs",
    "cap:sync": "npx cap sync && node scripts/patch-ios.mjs",
    "cap:open": "npx cap open ios",
    "analyze": "ANALYZE=true npm run build"
  },
